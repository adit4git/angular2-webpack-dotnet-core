        "@angular/compiler": "~5.0.4",
        "@angular/core": "~5.0.4",
        "@angular/forms": "~5.0.4",
        "@angular/http": "~5.0.4",
        "@angular/material": "^5.0.0-rc.2",
        "@angular/platform-browser": "~5.0.4",
        "@angular/platform-browser-dynamic": "~5.0.4",
        "@angular/router": "~5.0.4",
        "angular-in-memory-web-api": "~0.5.1",
        "bootstrap": "^3.3.7",
        "core-js": "^2.5.1",
        "jquery": "^3.2.1",
        "rxjs": "5.5.2",
        "zone.js": "^0.8.18"
