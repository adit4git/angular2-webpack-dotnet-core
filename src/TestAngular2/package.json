    "@angular/compiler": "~5.2.2",
    "@angular/core": "~5.2.2",
    "@angular/forms": "~5.2.2",
    "@angular/http": "~5.2.2",
    "@angular/material": "~5.2.0",
    "@angular/platform-browser": "~5.2.2",
    "@angular/platform-browser-dynamic": "~5.2.2",
    "@angular/router": "~5.2.2",
    "angular-in-memory-web-api": "~0.5.3",
    "core-js": "^2.5.3",
